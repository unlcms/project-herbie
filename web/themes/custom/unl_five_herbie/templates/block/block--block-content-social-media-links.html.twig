{{ attach_library('unl_five_herbie/social_media_links') }}
{% set attributes = attributes ?: create_attribute() %}
{% set social_media_classes = ['unlcms-component-spacer unlcms-socialmedia-component unl-bg-scarlet dcf-d-flex dcf-flex-wrap dcf-ai-center dcf-jc-center dcf-row-gap-5 dcf-col-gap-8'] %}

<div{{ attributes.addclass(social_media_classes) }}>
  {{ title_prefix }}
  {{ content.b_social_media_links_title }}
  {{ title_suffix }}

  <address>
    <ul class="dcf-list-bare dcf-d-flex dcf-flex-wrap dcf-ai-center dcf-jc-center dcf-col-gap-6 dcf-row-gap-5 dcf-txt-sm dcf-mb-0">
      {% if content.b_social_media_links_facebook[0]['#context']['value'] %}
      <li class="dcf-mb-0">
        {% set filtered_username_input = content.b_social_media_links_facebook[0]['#context']['value']|url_encode %}
        <a class="dcf-d-block dcf-h-6 dcf-w-6 unl-cream" href="https://www.facebook.com/{{filtered_username_input}}" aria-label="Our Facebook profile">
          <svg class="dcf-h-100% dcf-w-100% dcf-fill-current" focusable="false" height="16" width="16" viewBox="0 0 48 48">
            <path d="M48 24.1c0-13.3-10.7-24-24-24S0 10.9 0 24.1c0 12 8.8 21.9 20.2 23.7V31.1h-6.1v-6.9h6.1v-5.3c0-6 3.6-9.3 9.1-9.3 2.6 0 5.4.5 5.4.5V16h-3c-3 0-3.9 1.9-3.9 3.7v4.5h6.7l-1.1 6.9h-5.6v16.8C39.2 46.1 48 36.1 48 24.1z"></path>
          </svg>
        </a>
      </li>
      {% endif %}
      {% if content.b_social_media_links_twitter[0]['#context']['value'] %}

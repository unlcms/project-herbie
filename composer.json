{
    "name": "unlcms/project-herbie",
    "description": "Drupal at UNL (based on drupal/recommended-project)",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://github.com/unlcms/project-herbie",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "vcs",
            "url": "https://github.com/unlcms/project-herbie-composer-plugin.git"
        },
        {
            "type": "package",
            "package": {
                "name": "unl/wdntemplates",
                "version": "5.3",
                "source": {
                    "type": "git",
                    "url": "https://github.com/unl/wdntemplates.git",
                    "reference": "5.3"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "atomiks/tippyjs",
                "version": "5.2.1",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/atomiks/tippyjs/archive/refs/tags/v5.2.1.zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "cferdinandi/tabby",
                "version": "12.0.3",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/cferdinandi/tabby/archive/refs/tags/v12.0.3.zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "robinherbots/jquery.inputmask",
                "version": "5.0.5",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/RobinHerbots/Inputmask/archive/refs/tags/5.0.5.zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "jackocnr/intl-tel-input",
                "version": "16.1.0",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/jackocnr/intl-tel-input/archive/refs/tags/v16.1.0.zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "ractoon/jquery-text-counter",
                "version": "0.9.0",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/ractoon/jQuery-Text-Counter/archive/refs/tags/0.9.0.zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "nigelotoole/progress-tracker",
                "version": "2.0.6",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/NigelOToole/progress-tracker/archive/refs/tags/2.0.6.zip"
                }
            }
        }
    ],
    "require": {
        "php": ">=8.1",
        "atomiks/tippyjs": "^5.2",
        "cferdinandi/tabby": "12.0.3",
        "composer/installers": "^2.1",
        "cweagans/composer-patches": "^1.6.5",
        "drupal/admin_toolbar": "^3.0",
        "drupal/administerusersbyrole": "^3.3",
        "drupal/allowed_formats": "^2.0",
        "drupal/asset_injector": "^2.7",
        "drupal/auto_entitylabel": "^3.0@beta",
        "drupal/block_content_permissions": "^1.11",
        "drupal/block_form_alter": "^2.0",
        "drupal/cas": "^2.0",
        "drupal/codemirror_editor": "^1.5",
        "drupal/config_filter": "^1.0",
        "drupal/config_ignore": "^2.4",
        "drupal/config_split": "^1.4",
        "drupal/config_update": "^2.0@alpha",
        "drupal/core-composer-scaffold": "^10",
        "drupal/core-project-message": "^10",
        "drupal/core-recommended": "^10",
        "drupal/crop": "^2.1",
        "drupal/ctools": "^4.0",
        "drupal/default_content": "^2.0@alpha",
        "drupal/diff": "^1.0",
        "drupal/entity_reference_revisions": "^1.0",
        "drupal/entity_share": "^3.0@RC",
        "drupal/entity_share_cron": "^3.0",
        "drupal/entity_usage": "^2.0",
        "drupal/external_entities": "2.x-dev@dev",
        "drupal/features": "5.0.x-dev@dev",
        "drupal/feeds": "^3.0@beta",
        "drupal/feeds_ex": "^1.0@beta",
        "drupal/field_group": "^3.0",
        "drupal/file_delete": "^2.0",
        "drupal/focal_point": "2.x-dev@dev",
        "drupal/heading": "^1.7",
        "drupal/honeypot": "^2.1",
        "drupal/image_class": "^2.0",
        "drupal/image_effects": "^3.3",
        "drupal/imagemagick": "^3.3",
        "drupal/inline_block_title_automatic": "^1.1",
        "drupal/inline_entity_form": "^1.0",
        "drupal/layout_builder_component_attributes": "^2.1",
        "drupal/layout_builder_iframe_modal": "^1.3",
        "drupal/layout_builder_modal": "^1.2",
        "drupal/layout_builder_operation_link": "^2.0",
        "drupal/layout_builder_restrictions": "^2.0",
        "drupal/layout_builder_styles": "^2.0",
        "drupal/linkit": "^6.0@beta",
        "drupal/linkit_media_library": "^1.0@alpha",
        "drupal/markup": "^2.0@beta",
        "drupal/media_entity_file_replace": "^1.1",
        "drupal/media_file_delete": "^1.3",
        "drupal/menu_admin_per_menu": "^1.0",
        "drupal/menu_block": "^1.5",
        "drupal/metatag": "^1.22",
        "drupal/oembed_providers": "^2.1",
        "drupal/paragraphs": "^1.8",
        "drupal/pathauto": "^1.3",
        "drupal/queue_ui": "^3.1",
        "drupal/rabbit_hole": "^1.0@beta",
        "drupal/redirect": "^1.5",
        "drupal/select2": "^1.13",
        "drupal/seven": "^1.0@alpha",
        "drupal/stable": "^2.0",
        "drupal/token": "^1.0",
        "drupal/twig_tweak": "^3.2",
        "drupal/twig_ui": "^2.0",
        "drupal/webform": "^6.2@beta",
        "drush/drush": "^11.5",
        "jackocnr/intl-tel-input": "16.1.0",
        "nigelotoole/progress-tracker": "^2.0",
        "npm-asset/select2": "^4.0",
        "oomphinc/composer-installers-extender": "^2.0",
        "ractoon/jquery-text-counter": "0.9.0",
        "robinherbots/jquery.inputmask": "5.0.5",
        "unl/wdntemplates": "^5.3",
        "unlcms/external_entities_unldirectory": "^2.0",
        "unlcms/project-herbie-composer-plugin": "^1.0",
        "unlcms/unl_breadcrumbs": "^1.0",
        "unlcms/unl_cas": "^2",
        "unlcms/unl_five": "^10",
        "unlcms/unl_multisite": "^1.1",
        "unlcms/unl_user": "^1.1",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "zaporylie/composer-drupal-optimizations": "^1.1"
    },
    "require-dev": {
        "drupal/config_inspector": "^2.1",
        "drupal/core-dev": "^10"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "drupal/console-extend-plugin": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "oomphinc/composer-installers-extender": true,
            "unlcms/project-herbie-composer-plugin": true,
            "zaporylie/composer-drupal-optimizations": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "drupal/core-project-message": true,
            "phpstan/extension-installer": true
        }
    },
    "autoload": {
        "classmap": [
            "scripts/composer/UnlScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "post-drupal-scaffold-cmd": [
            "cd web && patch -p1 < ../patches/custom-index.php.patch",
            "cd web && patch -p1 < ../patches/custom-htaccess.patch",
            "cd web && patch -p1 < ../patches/custom-htaccess-enforce-https.patch",
            "cd web && patch -p1 < ../patches/custom-htaccess-session-save_path.patch",
            "cd web && patch -p1 < ../patches/custom-htaccess-session-slash.patch",
            "cd web && patch -p1 < ../patches/custom-unl_multisite-example.sites.php.patch"
        ],
        "install-wdn": "DrupalProject\\composer\\UnlScriptHandler::deployWdn"
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-types": ["bower-asset", "npm-asset"],
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"],
            "web/modules/custom/{$name}": ["type:drupal-custom-module"],
            "web/themes/custom/{$name}": ["type:drupal-custom-theme"],
            "web/libraries/tippyjs/5.x": ["atomiks/tippyjs"],
            "web/libraries/tabby": ["cferdinandi/tabby"],
            "web/libraries/jquery.inputmask": ["robinherbots/jquery.inputmask"],
            "web/libraries/jquery.intl-tel-input": ["jackocnr/intl-tel-input"],
            "web/libraries/jquery.textcounter": ["ractoon/jquery-text-counter"],
            "web/libraries/progress-tracker": ["nigelotoole/progress-tracker"]
        },
        "patches": {
            "drupal/core": {
                "2827910 - Add Option to Display Top Level page in BookNavigationBlock": "patches/allow-top-item-to-show-2827910-54.patch",
                "3056149 - Include 'is_active' value in book tree items": "patches/book_tree_active_item-3056149-29.patch",
                "3060292 - Drupal\\media\\Entity\\Media::prepareSave should convert URL object metadata to string before saving" : "patches/drupal_media-resource_convert_url_object_to_string-3060292-30-D9.patch",
                "502430 - Allow the book outline functionality on non-book-enabled node types to be hidden": "patches/502430-90.patch",
                "Custom - Allow '@' in CSS selectors for Views displays": "patches/custom-views-DisplayPluginBase-valid-css-selector.patch",
                "2897377 - data-drupal-selector is set to random value": "patches/2897377-37.patch",
                "Custom (gh#366) - ProxySQL bug sidestep: don't release savepoints during site install": "patches/unlcms-366-dont-release-savepoint-on-install.patch"
            },
            "drupal/block_content_permissions": {
                "2920739 - Allow accessing the 'Custom block library' page without 'Administer blocks' permission": "patches/2920739-55.access_listing_page.patch"
            },
            "drupal/config_ignore": {
                "2857247 - Support for export filtering via Drush": "patches/config_ignore_2857247-75.patch"
            },
            "drupal/feeds": {
                "Custom (gh#365) - Remove Url creation from feeds_install which breaks site installation on prod": "patches/unlcms-365-feeds-remove-url-creation.patch"
            },
            "drupal/heading": {
                "3354814 - Configuration schema is not complete": "patches/3354814-heading-config-schema.patch"
            },
            "drupal/layout_builder_iframe_modal": {
                "3316931 - Add config schema for custom routes": "patches/layout_builder_iframe_modal-add_config_schema_custom_routes-3316931-3.patch"
            },
            "drupal/paragraphs": {
                "3090200 - Paragraphs do not render: access check for view fail when using layout builder": "patches/3084934-13_combine_3090200-22-update_0.patch"
            },
            "drush/drush": {
                "Custom - Do not allow config:import to be run": "patches/drush-config-import-message.patch"
            }
        },
        "drupal-scaffold": {
            "file-mapping": {
                "[web-root]/sites/default/default.settings.php": {
                  "append": "patches/default.settings.php.append.php"
                }
            },
            "locations": {
                "web-root": "web/"
            },
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        }
    }
}
